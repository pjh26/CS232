]0;root@mradmin-VirtualBox: /home/mradmin/Desktop/CS232 Testingroot@mradmin-VirtualBox:/home/mradmin/Desktop/CS232 Testing# cat newSysCalls.h
/** newSysCalls.h - Defines the wrappers for the sys calls created for Project 5
*
*	Students:	Nate Gamble
*			Peter Haagsma
*	Date:		04/30/2019
*
*/


#include <unistd.h>
#include <sys/syscall.h>

#define SYS_PRINTNAME 333
#define SYS_FILLTEAMARRAY 334
#define SYS_FILLUSERARRAY 335

static inline
long printName(void) {
	return syscall(SYS_PRINTNAME);
}

static inline
long fillTeamArray(char * buf, int n) {
	return syscall(SYS_FILLTEAMARRAY, buf, n);
}

static inline
long fillUserArray(char * buf, int n) {
	return syscall(SYS_FILLUSERARRAY, buf, n);
}
]0;root@mradmin-VirtualBox: /home/mradmin/Desktop/CS232 Testingroot@mradmin-VirtualBox:/home/mradmin/Desktop/CS232 Testing# cat main.cpp
/** main.cpp tests the new system calls created for Project 5
*
*	Students:	Nate Gamble
*			Peter Haagsma
*	Date:		04/30/2019
*
*
*/

#include <stdio.h>
#include "newSysCalls.h"

int main(int argc, char * argv[]) {

	printf("Invoking print name call...\n");

	long result = printName();

	printf("System call returned %ld\n", result);


	printf("\nInvoking fill team array call...\n");

	char myBuf[25] = "";
	int len = 25;
	result = fillTeamArray(myBuf, len);

	printf("System call returned %ld\n", result);
	printf("Buffer now reads\n %s \n", myBuf);


	printf("\nInvoking fill user array call...\n");

	char newBuf[15] = "";
	len = 15;
	result = fillUserArray(newBuf, len);

	printf("System call returned %ld\n", result);
	printf("Buffer now reads\n %s \n", newBuf);


	return 0;
}
